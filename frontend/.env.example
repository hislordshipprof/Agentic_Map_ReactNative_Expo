# API Configuration
API_BASE_URL=http://localhost:3000/api/v1

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Environment
NODE_ENV=development

# Backend API (for real device: use your PC's IPv4, e.g. http://10.0.0.144:3000/api/v1)
EXPO_PUBLIC_API_URL=

# ElevenLabs Voice Integration
# Set to 'true' to use ElevenLabs WebRTC voice (faster, ~100ms latency)
# Set to 'false' or leave empty to use legacy WebSocket voice
EXPO_PUBLIC_USE_ELEVENLABS=false

# ElevenLabs Agent ID (from ElevenLabs dashboard - Conversational AI)
# Required if EXPO_PUBLIC_USE_ELEVENLABS=true
EXPO_PUBLIC_ELEVENLABS_AGENT_ID=
